{
  "expo": {
    "name": "MayoClinic",
    "slug": "MayoClinic",
    "version": "2.2.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "mayoclinic",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.cliffordmukosh.mayoClinic"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-sqlite",
      "expo-secure-store",
      "expo-asset"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "9fa7f245-8d33-4be0-901f-7325c91e7c39"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "owner": "cliffordmukosh"
  }
}
